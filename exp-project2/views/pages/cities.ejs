<%- include("../includes/header") %>
<div class="container" style="min-height: 600px;">
    <div class="row">
        <div class="col-md-12">
            <h2>All Cities</h2>
            <table class="table table-dark table-hover table-striped">
                <tr>
                    <th>S.No.</th>
                    <th>Name</th>
                    <th>State</th>
                </tr>
                <% data.forEach((value, index)=>{ %>
                    <tr>
                        <td><%= index+1 %></td>
                        <td><%= value.name %></td>
                        <td><%= value.state %></td>
                    </tr>
                <% }) %>
                
            </table>
        </div>
    </div>
</div>
<%- include("../includes/footer") %>