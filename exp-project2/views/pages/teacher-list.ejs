<%- include("../includes/header") %>
<div class="container my-5" style="min-height: 600px;">
    <div class="row">
        <div class="col-md-12">
            <h3>Teacher-List Page</h3>
            <table class="table table-info">
                <tr>
                    <th>S.No.</th>
                    <th>Name</th>
                    <th>Class</th>
                    <th>Education</th>
                </tr>
                <% list.forEach((obj, index)=>{ %>
                    <tr>
                        <td><%= index+1 %></td>
                        <td><%= obj.fullname %></td>
                        <td><%= obj.class %></td>
                        <td><%= obj.education %></td>
                    </tr>
                <% }) %>

            </table>
        </div>

    </div>
</div>
<%- include("../includes/footer") %>