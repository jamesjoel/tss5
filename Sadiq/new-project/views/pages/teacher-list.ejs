<%- include("../includes/header") %>

<div class="container">
    <div class="row">
        <div class="col-md-10 offset-md-1">
            <div class="card my-4">
                <div class="card-header bg-primary text-center"><h3 class="text-light">Teacher-List</h3></div>
                <div class="card-body">
                    <table class="table table-info table-hover">
                        <tr>
                            <th>S.No.</th>
                            <th>NAME</th>
                            <th>CLASS</th>
                            <th>EDUCATION</th>
                            <th>FULL DETAIL</th>
                        </tr>
                        <% data.forEach((value, index)=>{ %>
                            <tr>
                                <td><%= index+1 %></td>
                                <td><%= value.fullname %></td>
                                <td><%= value.class %></td>
                                <td><%= value.education %></td>
                                <td><a class="btn btn-outline-info btn-sm" href="/teacher/list/view/<%= value._id %>">View</a></td>
                            
                            </tr>
                        <% }) %>
                    </table>
                </div>
                </div>
            </div>
        </div>
    </div>
</div>


<%- include("../includes/testimonial") %>
<%- include("../includes/footer") %>