
<%- include("../includes/header") %>
<br/>
  <div class="offset-md-5 my-3">
<a href="/student/add" class="btn btn-sm btn-info">+ADD</a>
</div>
<br/> 
<div class="container" style="min-height: 600px;">
    <div class="row">
      <div class="col-md-8 offset-md-2">

<table class="table table-bordered table stripped table-dark">
    <tr>
        <td> S.no </td>
        <td> Name </td>
        <td> Address </td>
        <td> Fees </td>
        <td> Delete</td>
        <td> Edit</td>
        <td> View</td>

    </tr>
    <% data.forEach((value, index)=>{ %>
     <tr>
        <td><%= index+1 %></td>
        <td><%= value.name %></td>
        <td><%= value.address %></td>
        <td><%= value.fee %></td>
        <td><a href="/student/delete/<%= value._id %>" class="btn btn-sm btn-danger">Delete</a></td>
        <td><a href="/student/edit/<%= value._id %>" class="btn btn-sm btn-primary">Edit<i class="fa fa-pencil" aria-hidden="true"></i></a></td>
        <td><a href="/student/view/<%= value._id %>" class="btn btn-sm btn-danger">View<i class="fa fa-eye" aria-hidden="true"></i></a></td>
     </tr>   

   <% }) %>
</table>
</div>
    </div>
    </div>

    <%- include("../includes/footer") %>
    
  